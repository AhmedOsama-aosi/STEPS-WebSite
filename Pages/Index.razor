     @code{
    private IJSObjectReference? aos;


    protected override async Task OnAfterRenderAsync(bool firstRender)
    {
        if (firstRender)
        {
            aos = await jsRuntime.InvokeAsync<IJSObjectReference>("import", "./Scripts/aos.js");
            if (aos != null)
            {
                await aos.InvokeVoidAsync("AosInit");
                await aos.InvokeVoidAsync("navClick");
               
            }


        }
    }
     }